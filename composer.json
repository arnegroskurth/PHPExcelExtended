{
  "name": "arne-groskurth/phpexcel-extended",
  "description": "PHPExcelExtended is a wrapper for the PHPOffice/PHPExcel package providing common functionality.",
  "keywords": ["phpexcel", "wrapper", "xlsx", "excel", "spreadsheet"],
  "homepage": "https://github.com/arnegroskurth/PHPExcelExtended",
  "authors": [
    {
      "name": "Arne Groskurth",
      "email": "arne.groskurth@gears9.de"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^5.4|^7.0",
    "phpoffice/phpexcel": "1.*",
    "arne-groskurth/temp-file": "^1.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5"
  },
  "suggest": {
    "symfony/http-foundation": "^3.1",
    "tecnickcom/tcpdf": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "ArneGroskurth\\PHPExcelExtended\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArneGroskurth\\PHPExcelExtended\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "serve-examples": "php -S localhost:8000 -t ./examples"
  },
  "config": {
    "process-timeout": 0
  }
}
